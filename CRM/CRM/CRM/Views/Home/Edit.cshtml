@model CRM.Models.ProfileDataModel

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<!DOCTYPE html>

<html>
<head>
    <title>Edit your Response</title>
</head>
<body>
@using (Html.BeginForm("Edit", "Home", FormMethod.Post))
{
    <div class="row">
        <br/>
        <br/>
        <table style="width: 50%" align="left">
            <tr>
                <th>@Html.LabelFor(a => a.FName)</th>


                <td>@Html.TextBoxFor(a => a.FName)
                   @Html.ValidationMessageFor(a=>a.FName) </td>
            </tr>
            <tr>
                <th>@Html.LabelFor(a => a.LName)</th>

                <td>
                    @Html.TextBoxFor(a => a.LName)
                   @Html.ValidationMessageFor(a => a.LName)
                 </td>
            </tr>
            <tr>
                <th>@Html.LabelFor(a => a.PhoneNo)</th>

                <td>
                    @Html.TextBoxFor(a => a.PhoneNo)
                    @Html.ValidationMessageFor(a => a.PhoneNo)
                            </td>
            </tr>
            <tr>
                <th>@Html.LabelFor(a => a.Email)</th>

                <td>
                    @Html.TextBoxFor(a => a.Email)
                    @Html.ValidationMessageFor(a => a.Email)
                 </td>
            </tr>


        </table>


    </div>
    <br/>

    @Html.ActionLink("save", "UpdateResult", new{ model = Model },null)
    @*<button id="btnsave">Save</button>
    <script type="text/javascript">
        document.getElementById("btnsave").onclick = function () {
        };
    </script>*@

   
}
<button id="myButton">Home</button>

<script type="text/javascript">
    document.getElementById("myButton").onclick = function() {

        location.href = "../Index";
    };
</script>
</body>
</html>


